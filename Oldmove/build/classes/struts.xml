	</action>

	<action name="SecondsetunaAction" class="com.oldmove.action.SecondsetunaAction" method="execute">
	<result name="success">setunasecond.jsp</result>
	</action>

	<action name="ThirdsetunaAction" class="com.oldmove.action.ThirdsetunaAction" method="execute">
	<result name="success">setunathird.jsp</result>
	</action>

	<action name="FourthsetunaAction" class="com.oldmove.action.FourthsetunaAction" method="execute">
	<result name="success">setunafourth.jsp</result>
	</action>

	<action name="FifthsetunaAction" class="com.oldmove.action.FifthsetunaAction" method="execute">
	<result name="success">setunafifth.jsp</result>
	</action>

	<action name="SetunaAction" class="com.oldmove.action.SetunaAction" method="execute">
	<result name="success">setunatitle.jsp</result>
	</action>

	</package>
</struts>