<struts>
	<constant name="struts.devMode" value ="true"/>
	<package name ="com.oldmove.action" extends="struts-default">

	<action name="FightAction" class="com.oldmove.action.FightAction" method="execute">
	<result name="success">fight.jsp</result>
	</action>

	<action name="SelectmonsterAction" class="com.oldmove.action.SelectmonsterAction" method="execute">
	<result name="success">selectmonster.jsp</result>
	</action>

	<action name ="LeadyAction" class ="com.oldmove.action.LeadyAction" method = "execute">
	<result name="success">leady.jsp</result>
	</action>

	<action name ="BattleAction" class="com.oldmove.action.BattleAction" method="execute">
	<result name ="success">battle.jsp</result>
	<result name ="input">winner.jsp</result>
	</action>

	<action name ="BattlefinishAction" class="com.oldmove.action.BattlefinishAction" method="execute">
	<result name ="success">index.jsp</result>
	</action>

	<action name ="GohomeAction" class="com.oldmove.action.GohomeAction" method="execute">
